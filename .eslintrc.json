{
  "parser": "@typescript-eslint/parser",
  "extends": ["react-app"],
  "rules": {
    "no-use-before-define": [0],
    "@typescript-eslint/no-use-before-define": [0]
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": { "typescript": {} }
  }
}