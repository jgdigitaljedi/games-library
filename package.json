{
  "name": "game-library",
  "version": "0.3.0",
  "homepage": "http://ghome.help/gameslib",
  "scripts": {
    "start": "npm run start:server & npm run start:ui",
    "start:ui": "npm run watch-css & node scripts/start.js",
    "start:server": "NODE_ENV=development nodemon server/server.js --ignore 'server/db/*'",
    "build": "NODE_ENV=production & node-sass-chokidar src/App.scss -o src/ & node scripts/build.js",
    "build-css": "node-sass-chokidar src/App.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/App.scss -o src/ --watch --recursive",
    "test": "node scripts/test.js",
    "everdrive": "node scripts/fixEverdrives.js",
    "combine:everdrives": "node server/extra/combineEverdrives.js",
    "migrateGames": "node libraryScripts/migrateToNewDataStructure/migrateGames.js",
    "process:newGames": "node libraryScripts/addNewWhileUnderConstruction/addNew.js",
    "gameeye": "node server/gameEye/convertCsv.js"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@reach/router": "^1.3.4",
    "@svgr/webpack": "^5.5.0",
    "@types/react-transition-group": "^4.4.4",
    "axios": "^0.26.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^9.1.2",
    "body-parser": "^1.20.0",
    "camelcase": "^6.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^4.1.2",
    "chart.js": "^3.7.1",
    "classnames": "^2.3.1",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "css-loader": "3.4.2",
    "csv-parse": "^4.16.3",
    "csv-parser": "^3.0.0",
    "diskdb": "^0.1.17",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "express": "^4.17.3",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "helmet": "^4.6.0",
    "html-webpack-plugin": "^4.5.2",
    "identity-obj-proxy": "3.0.0",
    "igdb-api-node": "^5.0.1",
    "jest": "^27.5.1",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "^27.5.1",
    "jest-watch-typeahead": "^0.6.5",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "0.9.0",
    "moment": "^2.29.2",
    "morgan": "^1.10.0",
    "node-sass-chokidar": "^1.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "pnp-webpack-plugin": "^1.7.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "primeicons": "^4.1.0",
    "primereact": "^6.5.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^5.1.0",
    "react-redux": "^7.2.8",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "request": "^2.88.2",
    "resolve": "^1.22.0",
    "resolve-url-loader": "^3.1.4",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "stringman": "^0.13.3",
    "stringman-utils": "^1.8.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.8",
    "ts-pnp": "^1.2.0",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "^3.11.3",
    "webpack-manifest-plugin": "2.2.0",
    "winston": "^3.7.2",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^16.11.26",
    "@types/reach__router": "^1.3.10",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-redux": "^7.1.23",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "bluebird": "^3.7.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^1.7.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.5.0",
    "**/@typescript-eslint/parser": "^4.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
